{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "compilerOptions": {
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.ui.tsbuildinfo",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/ui/*"
            ]
        }
    },
    "include": [
        "src/ui/**/*.ts",
        "src/ui/**/*.tsx",
        "src/ui/**/*.vue"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}